{
    "name": "rawphp/raw-datetime",
    "type": "library",
    "description": "RawDateTime is a wrapper with useful utility functions around the DateTime class and is used by RawPHP framework and other applications.",
    "keywords": [ "date", "datetime" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawDateTime\\": "src/RawPHP/RawDateTime/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
